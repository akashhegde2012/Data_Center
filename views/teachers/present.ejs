<%- include("../partials/header") %>
<%- include("../partials/teacherSidebar") %>

<link rel="stylesheet" href="/stylesheet/Layout.css" />
<link rel="stylesheet" href="/stylesheet/teachers.css">
<div class="content">
    <div class="container">
        <table class="example-table">
            <thead>
                <tr>
                    <td>No</td>
                    <td>Name</td>
                    <td>Id (Aadhar number)</td>
                    <td>Email</td>
                    <td>Designation</td>
                    <td>Date of Joining</td>
                </tr>
            </thead>
            <tbody>
                <%var i = 1;teachers.forEach(teacher=>{%>
                    <tr>
                        <td><%=i%></td>
                        <td><%=teacher.Name%></td>
                        <td><%=teacher['ID_number_Aadhar_number']%></td>
                        <td><%=teacher.Email%></td>
                        <td><%=teacher.Designation%></td>
                        <td><%=teacher.Date_of_joining%></td>
                    </tr>    
                    
                <%i++;})%>
            </tbody>
            <button type='button' class="btn btn-primary" id="generateExcel">Download</button>

        </table>
    </div>
</div>
<script src="/javascript/excel.js"></script>

<%- include("../partials/footer") %>
