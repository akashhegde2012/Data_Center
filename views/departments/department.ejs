<div class="content">
    <table class="transpose example-table" id="example-table">
        <thead>
        <th>
            <% for (var heading in details[0]){%>
            <td class="heading"><%=heading%></td>
            <%}%>
        </th>
        </thead>
        <tbody>
        <%details.forEach((detail)=>{%>
        <tr>
            <%for (const heading in detail){%>
            <td><%if(detail[heading] === null){%>
                Na
                <%}%>
                <%=detail[heading]%>
            </td>
            <%}%>
        </tr>
    </tbody>

        <%})%>
    </table>
